{
  "name": "launchpad-mobx",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --port=3737",
    "test": "standard 'src/**/*.js' --verbose | snazzy && jest --coverage --verbose",
    "build": "webpack -p"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "dotenv": "^4.0.0",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "mobx-utils": "^3.2.1",
    "moment": "^2.19.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "afterEach",
      "beforeEach",
      "describe",
      "expect",
      "fdescribe",
      "fit",
      "it",
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "devDependencies": {
    "async": "^2.5.0",
    "autoprefixer": "^7.1.5",
    "aws-sdk": "^2.134.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "mime": "^2.0.3",
    "mobx-react-devtools": "^4.2.15",
    "postcss-loader": "^2.0.8",
    "react-addons-test-utils": "^15.6.2",
    "sass-loader": "^6.0.6",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2"
  }
}
